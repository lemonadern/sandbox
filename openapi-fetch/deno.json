{
  "tasks": {
    "dev": "deno run --watch main.ts",
    "esm:add": "deno run -A https://esm.sh/v135 add",
    "esm:update": "deno run -A https://esm.sh/v135 update",
    "esm:remove": "deno run -A https://esm.sh/v135 remove",
    "gen:type": "deno run --allow-read --allow-write npm:openapi-typescript ./openapi.json -o ./generated-types.d.ts "
  },
  "imports": {
    "openapi-fetch": "https://esm.sh/v135/*openapi-fetch@0.9.5",
    "openapi-fetch/": "https://esm.sh/v135/*openapi-fetch@0.9.5/",
    "openapi-typescript": "https://esm.sh/v135/*openapi-typescript@6.7.5",
    "openapi-typescript/": "https://esm.sh/v135/*openapi-typescript@6.7.5/"
  },
  "scopes": {
    "https://esm.sh/v135/": {
      "ansi-colors": "https://esm.sh/v135/ansi-colors@4.1.3",
      "fast-glob": "https://esm.sh/v135/fast-glob@3.3.2",
      "js-yaml": "https://esm.sh/v135/js-yaml@4.1.0",
      "openapi-typescript-helpers": "https://esm.sh/v135/openapi-typescript-helpers@0.0.8",
      "supports-color": "https://esm.sh/v135/supports-color@9.4.0",
      "undici": "https://esm.sh/v135/undici@5.28.4",
      "yargs-parser": "https://esm.sh/v135/yargs-parser@21.1.1"
    }
  }
}
