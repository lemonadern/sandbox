{
  "openapi": "3.0.0",
  "info": {
    "title": "File Upload API",
    "version": "1.0.0",
    "description": "API for uploading files"
  },
  "paths": {
    "/upload": {
      "post": {
        "summary": "Upload a file",
        "operationId": "uploadFile",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary",
                    "description": "The file to upload"
                  },
                  "name": {
                    "type": "string",
                    "description": "The name of the file"
                  },
                  "query": {
                    "type": "object",
                    "properties": {
                      "version": {
                        "type": "integer"
                      }
                    },
                    "required": ["version"]
                  }
                },
                "required": ["file", "name", "query"]
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "File uploaded successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "File uploaded successfully"
                    }
                  },
                  "required": ["message"]
                }
              }
            }
          },
          "400": {
            "description": "Invalid file",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "Invalid file format"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
